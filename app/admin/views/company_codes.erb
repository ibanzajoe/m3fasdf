<% form_for :company, "/admin/company/save/#{params[:id]}", :id => 'admin-form', :multipart => true do |f| %>
<div class="dashhead">
  <div class="dashhead-titles">
    <h6 class="dashhead-subtitle">Dashboards</h6>
    <h2 class="dashhead-title">Promo Codes</h2>
  </div>
  <div class="btn-toolbar dashhead-toolbar">
    <% if !params[:id].nil? %>
    <!-- <a href="/admin/company/import_codes" class="btn btn-primary">Import Codes</a> -->
    <a href="/admin/company/<%= params[:id] %>" class="btn btn-primary-outline">Go Back</a>
    <% end %>
    <!-- <button class="btn btn-primary-outline">Save Codes</button> -->
  </div>
</div>
<hr class="m-t">
<div class="row">
  <div class="col-md-6">
    <h4>List All</h4>
    <section class="content-header bd-example">
      <table class="table table-sm">
        <thead>
          <tr>
            <th>Code</th>
            <th>Used</th>
            <th>Date</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">1</th>
            <td>HLH96</td>
            <td>14</td>
          </tr>          
        </tbody>
      </table>
    </section>
  </div>
  <div class="col-md-6">
    <h4>Import Promo Codes</h4>
    <section class="content-header bd-example">
      <div class="field row" id="field_company">
        <div class="col-md-12">
          <div class="form-group">
            <%= f.label :code, :caption => "Code", :required => true %>
            <%= f.text_area :code, :class => 'form-control', :rows => 10, :required => true, :placeholder => 'JOEFREERIDE' %>
            </div><!-- /.form-group -->
            </div><!-- /.col -->
            </div><!-- /.row -->
            <button class="btn btn-primary-outline">Add</button>
            <!-- /.box -->
          </section>
          
        </div>
      </div>
      <% end %>